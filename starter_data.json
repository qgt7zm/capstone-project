  {
    "model": "myapp.element",
    "pk": 1,
    "fields": {
      "name": "Badges",
      "description": "A trophy awarded for reaching a certain milestone. Also known as Achievements. Commonly grouped into Points, Badges, and Leaderboards (PBL)."
    }
  },
  {
    "model": "myapp.element",
    "pk": 2,
    "fields": {
      "name": "Leaderboards",
      "description": "A list of players in order of how much they have achieved. Also known as Rankings or Scoreboards. Commonly grouped into Points, Badges, and Leaderboards (PBL)."
    }
  },
  {
    "model": "myapp.element",
    "pk": 3,
    "fields": {
      "name": "Points",
      "description": "A numerical counter of achievement, gained or lost by actions. Also known as Score or Experience Points (XP). Commonly grouped into Points, Badges, and Leaderboards (PBL)."
    }
  },
  {
    "model": "myapp.element",
    "pk": 4,
    "fields": {
      "name": "Avatar",
      "description": "A customizable user profile picture. Users may equip their avatar with clothing items earned by performing tasks. Users may also give themselves a pseudonym or username."
    }
  },
  {
    "model": "myapp.element",
    "pk": 5,
    "fields": {
      "name": "Narrative",
      "description": "A continuous storyline that can persist throughout multiple lessons assignments, often with instructors or students as characters. Also known as Story."
    }
  },
  {
    "model": "myapp.element",
    "pk": 6,
    "fields": {
      "name": "Levels",
      "description": "Milestones that players can reach by collecting enough points. Levels may unlock new actions or content for players. Also known as Ranks."
    }
  },
  {
    "model": "myapp.element",
    "pk": 7,
    "fields": {
      "name": "Currency",
      "description": "In-game currency that users can collect and exchange for items of value."
    }
  },
  {
    "model": "myapp.element",
    "pk": 8,
    "fields": {
      "name": "Roleplaying",
      "description": "Allows players to imagine themselves as a fictional character in a story. Players may specialize into character classes with predefined actions."
    }
  },
  {
    "model": "myapp.element",
    "pk": 9,
    "fields": {
      "name": "Teamwork",
      "description": "Students collaborate together to achieve a common objective, rather than competing against each other."
    }
  },
  {
    "model": "myapp.outcome",
    "pk": 1,
    "fields": {
      "name": "Grades",
      "description": "Total points earned on assignments or assessments. Also known as accuracy or scores."
    }
  },
  {
    "model": "myapp.outcome",
    "pk": 2,
    "fields": {
      "name": "Speed",
      "description": "The time taken to respond to a question."
    }
  },
  {
    "model": "myapp.outcome",
    "pk": 3,
    "fields": {
      "name": "Social Interaction",
      "description": "How often students interact with other students."
    }
  },
  {
    "model": "myapp.outcome",
    "pk": 4,
    "fields": {
      "name": "Motivation",
      "description": "How willing students are to begin or continue learning. May be intrinsic of extrinsic."
    }
  },
  {
    "model": "myapp.outcome",
    "pk": 5,
    "fields": {
      "name": "Engagement",
      "description": "How much students are willing to participate in group or individual learning activities beyond what is necessary. Also known as retention."
    }
  },
  {
    "model": "myapp.outcome",
    "pk": 6,
    "fields": {
      "name": "Satisfaction",
      "description": "How much students feel like they enjoy learning."
    }
  },
  {
    "model": "myapp.outcome",
    "pk": 7,
    "fields": {
      "name": "Perceived Competence",
      "description": "How much students believe they are succeeding with learning. Also known as learner empowerment."
    }
  },
  {
    "model": "myapp.outcome",
    "pk": 8,
    "fields": {
      "name": "Autonomy",
      "description": "Students feel more independence and control over their own actions."
    }
  },
